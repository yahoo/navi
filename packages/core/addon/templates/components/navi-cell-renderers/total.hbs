{{get data column.type}}
{{#if data.__meta__.hasPartialData}}
  <div class="table-cell__info-message">
    {{navi-icon "exclamation-triangle" class="table-cell__info-message--icon"}}
    {{#ember-tooltip side="right" class="table-cell__info-message--tooltip" enableLazyRendering=true}}
      Totals and Subtotals are not calculated when data is partially loaded
    {{/ember-tooltip}}
  </div>
{{/if}}
