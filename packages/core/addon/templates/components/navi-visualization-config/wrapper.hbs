<div class="navi-visualization-config" ...attributes>
  {{#let (component (concat "navi-visualization-config/" @visualization.type)) as |VisualizationConfig|}}
    <VisualizationConfig
      @request={{@request}}
      @response={{@response}}
      @options={{@visualization.metadata}}
      @onUpdateConfig={{@onUpdateConfig}}
      @type={{@visualization.type}}
      class="navi-visualization-config__contents"
    />
  {{/let}}
</div>
