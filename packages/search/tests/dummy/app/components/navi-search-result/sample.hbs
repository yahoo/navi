<table class="navi-search-results__item navi-search-results__item--striped" ...attributes>
  <tbody>
    {{#each @data as |result|}}
      <tr class="navi-search-sample-result">
        <td class="navi-search-sample-result__icon">
          <NaviIcon @icon="file-text" />
        </td>
        <td class="navi-search-sample-result__item">
          {{result}}
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>
