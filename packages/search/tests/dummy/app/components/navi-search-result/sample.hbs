<table class="table is-striped navi-search-result" ...attributes>
  <tbody>
    {{#each @data as |result|}}
      <tr class="navi-search-result__sample">
        <td class="navi-search-result__sample result_icon is-center">
          <span class="d-icon d-file-text is-grey-800"></span>
        </td>
        <td class="navi-search-result__sample result_element">
          {{result}}
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>
