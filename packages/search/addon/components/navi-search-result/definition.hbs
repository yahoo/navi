<table class="table navi-search-result" ...attributes>
  <tbody>
    {{#each @data as |result|}}
      <tr class='navi-search-result__asset'>
        <td class="navi-search-result__asset result_element">
            <h5>{{result.longName}}</h5>
            <p>{{result.description}}</p>
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>
