<table class="navi-search-results__item navi-search-results__item--striped" ...attributes>
  <tbody>
    {{#each this.results as |result|}}
      <tr class="navi-search-asset-result">
       <td class="navi-search-asset-result__icon">
           <NaviIcon @icon="{{if (eq result.type "report") "file-text"}}{{if (eq result.type "dashboard") "bar-chart"}}" />
        </td>
        <td class="navi-search-asset-result__item">
          <LinkTo @route={{result.route}} @model={{result.modelId}}>
            {{result.title}}
          </LinkTo>
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>
