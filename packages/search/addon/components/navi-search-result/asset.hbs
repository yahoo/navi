<ul class='navi-search-results' ...attributes>
  {{#each this.results as |result|}}
    <li class='navi-search-result__asset'>
      <LinkTo @route={{result.route}} @model={{result.modelId}}>
        {{result.title}}
      </LinkTo>
    </li>
  {{/each}}
</ul>
