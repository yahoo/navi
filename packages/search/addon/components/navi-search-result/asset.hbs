<table class="table is-striped navi-search-result" ...attributes>
  <tbody>
    {{#each this.results as |result|}}
      <tr class='navi-search-result__asset'>
       <td class="navi-search-result__asset result_icon is-center">
           <NaviIcon @icon="{{if (eq result.type "report") "file-text"}}{{if (eq result.type "dashboard") "bar-chart"}}" />
        </td>
        <td class="navi-search-result__asset result_element">
          <LinkTo @route={{result.route}} @model={{result.modelId}}>
            {{result.title}}
          </LinkTo>
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>
