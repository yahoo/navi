<table class="navi-search-results__item navi-search-results__item--striped navi-search-results__item--asset" ...attributes>
  <tbody class="navi-search-result__item-body">
    {{#each this.results as |result|}}
      <tr class="navi-search-asset-result">
       <td class="navi-search-asset-result__icon">
           <NaviIcon @icon={{result.icon}} />
        </td>
        <td class="navi-search-asset-result__item">
          <LinkTo @route={{result.route}} @model={{result.modelId}} @invokeAction={{@closeResults}}>
            {{result.title}}
          </LinkTo>
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>
