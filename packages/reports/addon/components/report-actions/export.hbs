<button
  type="button"
  class="export__action-btn"
  ...attributes
  {{on "click" (queue
    (fn this.showExportNotification)
    (perform this.getDownloadURLTask)
    (fn this.downloadURLLink)
  )}}
  disabled={{@disabled}}
>
{{yield}}
</button>
