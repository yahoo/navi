{{#let (model-for (parent-route)) as | report |}}
  <NaviInfoMessage
    @icon="chart-upward-solid"
    @title="Get started by selecting some dimensions and metrics."
    class="navi-report-new__info-message p-20 m-10"
  >
    <DenaliButton
      @style="outline"
      class="navi-link clickable" type="button"
      {{on "click" (pipe
        (route-action "validate" report)
        (route-action "runReport" report)
      )}}>
      Run
    </DenaliButton>
  </NaviInfoMessage>
{{/let}}