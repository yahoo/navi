<DirTablePlaceholder
  class="dir-empty"
  @icon="folder-open"
  @title="Nothing Here Yet"
>
  <p>There currently is not anything in this collection.</p>
  {{#each-in (from-entries this.fileTypes) as |typeName fileType|}}
    <LinkTo @route={{fileType.linkRoute}}>
      <DenaliButton 
        @icon={{fileType.iconClass}}
        @style="text" 
      >
        New {{capitalize (singularize typeName) }}
      </DenaliButton>
    </LinkTo>
  {{/each-in}}
</DirTablePlaceholder>