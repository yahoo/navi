{{navi-dashboard
    dashboard=model.dashboard
    dataForWidget=model.dataForWidget
    saveAction=(route-action "saveDashboard")
    deleteAction=(item-action "DELETE_ITEM" model.dashboard)
    onUpdateFilter=(action "updateFilter")
    onRemoveFilter=(action "removeFilter")
    onAddFilter=(action "addFilter")
    onModelSaveOrRevert=(action "clearFilterQueryParams")
    addWidgetToDashboard=(action "addToDashboard")
    width=model.dashboard.presentation.columns
}}
