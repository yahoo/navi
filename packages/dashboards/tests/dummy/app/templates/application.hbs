<DenaliNavbar @isResponsive={{true}} as |Nav|>
  <Nav.Left as |Left|>
    <Left.LinkTo @route="dashboards">Navi Dashboards</Left.LinkTo>
  </Nav.Left>
  <Nav.Center as |Center|>
    <Center.LinkTo @route="dashboard-collections.collection" @model={{1}}>Collection 1</Center.LinkTo>
    <Center.LinkTo @route="dashboard-collections.collection" @model={{2}}>Collection 2</Center.LinkTo>
  </Nav.Center>
</DenaliNavbar>
{{outlet}}
<div class="navi-notifications">
  {{this.naviNotifications.options.message}} {{this.naviNotifications.options.type}}
</div>